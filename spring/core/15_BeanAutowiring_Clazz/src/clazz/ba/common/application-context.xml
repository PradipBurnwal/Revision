<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="robot" class="clazz.ba.beans.Robot" autowire="constructor"/>
	
	<bean id="chip" class="clazz.ba.beans.Chip">
		<property name="id" value="10"/>
		<property name="type" value="Microchip"/>
	</bean>
		
	<bean id="sensor" class="clazz.ba.beans.Sensor">
		<property name="id" value="100"/>
		<property name="range" value="25m"/>
	</bean>
	
</beans>
