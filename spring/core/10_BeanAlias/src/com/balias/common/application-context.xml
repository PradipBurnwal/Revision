<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="address" name="address1" class="com.balias.beans.Address">
		<property name="addressLine1" value="Sln Hostel"/>
		<property name="addressLine2" value="Roopan Agrahara"/>
		<property name="city" value="Bangaluru"/>
		<property name="state" value="Karnataka"/>
		<property name="zip" value="560056"/>
		<property name="country" value="India"/>
	</bean>
	<alias name="address1" alias="adr"/>
	<alias name="address" alias="location"/>

</beans>
