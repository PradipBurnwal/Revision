package com.blc.beans;

public class Robot {
	private String name;
	private SenserDriver driver;
	
	public Robot(SenserDriver driver) {
		this.driver = driver;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "Robot [name=" + name + ", driver=" + driver + "]";
	}
	
	
}
