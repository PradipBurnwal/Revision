package com.blc.beans;

public class Calculator {
	private int a;
	private int b;
	private int sum;
	public Calculator(int a) {
		this.a = a;
	}
	public int getB() {
		return b;
	}
	public void setB(int b) {
		this.b = b;
	}
	
	public void init(){
		System.out.println("init()");
		this.sum = this.a+this.b;
	}
}
