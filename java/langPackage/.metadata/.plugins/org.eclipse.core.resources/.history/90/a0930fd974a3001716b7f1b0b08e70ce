package com.itc.langStringPack;

public class Lab020Trim {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		String st1 = "ITCInfotech";
		String st2 = st1.trim();
		System.out.println(st1 == st2);
		System.out.println(st1+"\t"+st1.length()+"\t"+st1.isEmpty());
		System.out.println(st2+"\t"+st2.length()+"\t"+st2.isEmpty());
		System.out.println();
		String st3 = " ITCInfotech ";
		String st4 = st3.trim();
		System.out.println(st3 == st4);
		System.out.println(st3+"\t"+st3.length()+"\t"+st3.isEmpty());
		System.out.println(st4+"\t"+st4.length()+"\t"+st4.isEmpty());
		System.out.println();
		String st5 = "";
		System.out.println(st5.length());
		System.out.println(st5.isEmpty());
	}

}
